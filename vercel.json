{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/em/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/blogs/js/(.*)",
      "dest": "/public/em/blogs/js/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/em/js/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}